# Development Environment Configuration Example
# Copy this to .env.development and update values as needed

# Database Configuration (Dev postgres on port 5433)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=mixtape_battle_dev
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/mixtape_battle_dev

# NextAuth Configuration (Dev on localhost:3000)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-dev-secret-here

# Email Configuration (use same as production or test service)
EMAIL_SERVER_HOST=smtp.example.com
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER=your-email@example.com
EMAIL_SERVER_PASSWORD=your-password
EMAIL_FROM=your-email@example.com

# Node environment
NODE_ENV=development
